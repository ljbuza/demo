{
    "extends": "airbnb",
    "parserOptions": {
            "ecmaVersion": 6,
            "ecmaFeatures": {
                        "jsx": true
                    }
        },
    "env": {
            "jquery": true
        },
    "globals": {
            "fetch": true
        },
    "rules": {
            "react/prop-types": "off",
            "react/forbid-prop-types": 0
        },
    "plugins": [
            "react",
            "jsx-a11y",
            "import"
        ]
}
